{
  "homepage": "https://informatievlaanderen.github.io/OSLO-Playground/",
  "name": "oslo-playground2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "jest --coverage",
    "lint": "eslint --ext .js,.vue src --cache",
    "prepare": "husky install"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/polyfill": "7.12.1",
    "@govflanders/vl-ui-action-group": "3.12.29",
    "@govflanders/vl-ui-alert": "3.12.29",
    "@govflanders/vl-ui-button": "3.12.29",
    "@govflanders/vl-ui-content-header": "3.12.29",
    "@govflanders/vl-ui-core": "4.1.29",
    "@govflanders/vl-ui-data-table": "3.12.29",
    "@govflanders/vl-ui-dropdown-navigation": "3.12.29",
    "@govflanders/vl-ui-icon": "3.12.29",
    "@govflanders/vl-ui-input-field": "3.12.29",
    "@govflanders/vl-ui-introduction": "3.12.29",
    "@govflanders/vl-ui-select": "3.12.29",
    "@govflanders/vl-ui-side-navigation": "3.12.29",
    "@govflanders/vl-ui-tabs": "3.12.29",
    "@govflanders/vl-ui-textarea": "3.12.29",
    "@govflanders/vl-ui-titles": "3.12.9",
    "@govflanders/vl-ui-upload": "3.12.9",
    "@govflanders/vl-ui-vue-components": "3.11.0",
    "@jest/globals": "26.6.2",
    "@testing-library/jest-dom": "5.11.9",
    "core-js": "3.8.3",
    "js-base64": "2.6.4",
    "jsonld": "2.0.2",
    "jsonld-streaming-parser": "1.1.2",
    "n3": "1.8.0",
    "node-fetch": "2.6.1",
    "popper.js": "1.16.1",
    "rdf-parse": "1.7.0",
    "rdfxml-streaming-parser": "1.4.0",
    "sass": "1.35.1",
    "stream": "0.0.2",
    "streamify-string": "1.0.1",
    "vue": "2.6.12",
    "vue-router": "3.5.1",
    "vuex": "3.6.2",
    "xml-formatter": "1.2.1",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.11",
    "@vue/cli-service": "4.5.11",
    "@vue/test-utils": "1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "eslint": "7.30.0",
    "eslint-plugin-vue": "7.13.0",
    "fetch-mock": "9.11.0",
    "gh-pages": "3.1.0",
    "husky": "7.0.1",
    "jest": "26.6.3",
    "sass-loader": "^10.2.0",
    "vue-jest": "3.0.7",
    "vue-template-compiler": "2.6.12"
  }
}
